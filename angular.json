{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-bit-demo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "docs",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "node_modules/prismjs/themes/prism-okaidia.css",
              "node_modules/prismjs/plugins/toolbar/prism-toolbar.css",
              "node_modules/prismjs/plugins/autolinker/prism-autolinker.css",
              "node_modules/prismjs/plugins/command-line/prism-command-line.css",
              "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.css",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/plugins/toolbar/prism-toolbar.min.js",
              "node_modules/prismjs/plugins/autolinker/prism-autolinker.min.js",
              "node_modules/prismjs/plugins/autoloader/prism-autoloader.min.js",
              "node_modules/prismjs/plugins/command-line/prism-command-line.min.js",
              "node_modules/prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.min.js",
              "node_modules/prismjs/plugins/custom-class/prism-custom-class.min.js",
              "node_modules/prismjs/plugins/data-uri-highlight/prism-data-uri-highlight.min.js",
              "node_modules/prismjs/plugins/file-highlight/prism-file-highlight.min.js",
              "node_modules/prismjs/plugins/highlight-keywords/prism-highlight-keywords.min.js",
              "node_modules/prismjs/plugins/keep-markup/prism-keep-markup.min.js",
              "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.min.js",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.min.js",
              "node_modules/prismjs/components/prism-scss.min.js",
              "node_modules/prismjs/components/prism-bash.min.js",
              "node_modules/prismjs/components/prism-json.min.js",
              "node_modules/prismjs/components/prism-typescript.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-bit-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-bit-demo:build:production"
            }
          }
        }
      }
    },
    "ngx-bit-demo-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ngx-bit-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-bit-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-bit": {
      "root": "projects/ngx-bit",
      "sourceRoot": "projects/ngx-bit/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-bit/tsconfig.lib.json",
            "project": "projects/ngx-bit/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-bit/src/test.ts",
            "tsConfig": "projects/ngx-bit/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-bit/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-bit/tsconfig.lib.json",
              "projects/ngx-bit/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngx-bit-demo"
}
