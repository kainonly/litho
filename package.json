{
  "name": "console",
  "description": "基于 Antd 集成的 Angular 组件库，低代码前端的实验项目",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "start": "ng s --host 0.0.0.0 --disable-host-check",
    "build": "ng b --configuration production",
    "build:ng": "ng b angular --configuration production",
